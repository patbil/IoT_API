{
  "name": "raspi-pca9685-pwm",
  "version": "1.0.3",
  "description": "Hardware PWM by PCA9685 via I2C on the Raspberry Pi",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "run-s lint",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint",
    "jest": "jest --coverage && coveralls < coverage/lcov.info",
    "tslint": "tslint 'src/**/*.ts'"
  },
  "keywords": [
    "raspberry pi",
    "raspi-io",
    "i2c",
    "nodebots",
    "LED",
    "servo"
  ],
  "author": "Kiyo Chinzei",
  "license": "MIT",
  "contributors": [
    {
      "name": "Kiyo Chinzei",
      "email": "kchinzei@gmail.com"
    }
  ],
  "bundleDependencies": false,
  "config": {
    "unsafe-perm": true
  },
  "cpu": [
    "arm",
    "arm64"
  ],
  "dependencies": {
    "raspi-board": "^7.3.1",
    "raspi-i2c": "^6.2.4",
    "raspi-peripheral": "^3.0.4",
    "raspi-soft-pwm": "^6.0.2"
  },
  "deprecated": false,
  "devDependencies": {
    "@types/i2c-bus": "^5.1.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "eslint": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "coveralls": "^3.1.0"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "homepage": "https://github.com/kchinzei/raspi-pca9685-pwm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kchinzei/raspi-pca9685-pwm.git"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "types": "./declarations/index.d.ts"
}
